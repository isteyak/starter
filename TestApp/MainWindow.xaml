                    <RibbonTab Header="View" IsSelected="True">
                        <RibbonGroup DockPanel.Dock="Right">
                            <RibbonMenuButton
                                ac:ButtonExtensions.Icon="{StaticResource ACExport}"
                                IsEnabled="False"
                                Label="Export Report">
                                <RibbonMenuItem ac:ButtonExtensions.Icon="{StaticResource ACPdf}" Header="Export as PDF" />
                                <RibbonMenuItem ac:ButtonExtensions.Icon="{StaticResource ACWord}" Header="Export as Word" />
                                <RibbonMenuItem ac:ButtonExtensions.Icon="{StaticResource ACExcel}" Header="Export as Excel" />
                            </RibbonMenuButton>
                        </RibbonGroup>
                        <RibbonGroup>
                            <RibbonButton
                                ac:ButtonExtensions.Icon="{StaticResource ACNewFile}"
                                Click="Button_Click"
                                Label="Controls" />
                            <RibbonButton ac:ButtonExtensions.Icon="{StaticResource ACFolder}" Label="Open" />
                            <RibbonButton ac:ButtonExtensions.Icon="{StaticResource ACSave}" Label="Piping" />
                            <RibbonButton ac:ButtonExtensions.Icon="{StaticResource ACExport}" Label="Export" />
                            <RibbonMenuButton ac:ButtonExtensions.Icon="{StaticResource ACSave}" Label="Save">
                                <RibbonMenuItem
                                    Width="Auto"
                                    ac:ButtonExtensions.Icon="{StaticResource ACClose}"
                                    Header="Save" />
                                <!--<RibbonMenuItem Width="Auto" Header="Save As" />-->
                                <RibbonMenuItem>
                                    <RibbonMenuItem.Template>
                                        <ControlTemplate TargetType="{x:Type RibbonMenuItem}">
                                            <RibbonCheckBox Label="Test" />
                                        </ControlTemplate>
                                    </RibbonMenuItem.Template>
                                </RibbonMenuItem>
                            </RibbonMenuButton>
                            <RibbonMenuButton
                                Height="38"
                                ac:ButtonExtensions.Command="{Binding TestCommand}"
                                ac:ButtonExtensions.Enabled="False"
                                ac:ButtonExtensions.Icon="{StaticResource ACLayoutView}"
                                Label="LAYOUT_VIEW">
                                <RibbonMenuItem
                                    Command="{Binding LayoutViewChangedCommand}"
                                    CommandParameter="Normal"
                                    Header="NORMAL" />
                                <RibbonMenuItem
                                    Command="{Binding LayoutViewChangedCommand}"
                                    CommandParameter="BinaryTree"
                                    Header="BINARY_TREE"
                                    IsEnabled="{Binding IsCntrlVisible}" />
                                <RibbonMenuItem
                                    x:Name="DBT"
                                    Command="{Binding LayoutViewChangedCommand}"
                                    CommandParameter="SchemaA"
                                    Header="DOUBLE_BRANCH_BINARY_TREE"
                                    IsEnabled="{Binding IsCntrlVisible}" />
                            </RibbonMenuButton>
                            <RibbonButton ac:ButtonExtensions.Icon="{StaticResource ACClose}" Label="Close" />
                            <RibbonButton
                                ac:ButtonExtensions.Icon="{StaticResource ACRedo}"
                                IsEnabled="False"
                                Label="Redo" />
                            <RibbonButton
                                ac:ButtonExtensions.Icon="{StaticResource ACUndo}"
                                IsEnabled="False"
                                Label="Undo" />
                        </RibbonGroup>
                    </RibbonTab>
                    <RibbonTab Header="Test" />
                </Ribbon>

