                    IsChecked="True"
                    IsThreeState="True" />
                <CheckBox
                    x:Name="ShowHeaders"
                    Grid.Row="1"
                    Margin="200,0,0,0"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Content="Show" />

                <StackPanel
                    Grid.Row="2"
                    Margin="0,25,0,0"
                    Orientation="Vertical"
                    Visibility="{Binding ElementName=Switch, Path=IsChecked, Converter={StaticResource BooleanToVisibilityConverter}}">
                    <StackPanel Orientation="Horizontal">
                        <StackPanel HorizontalAlignment="Left" Orientation="Vertical">
                            <TextBlock Text="Primary Buttons" />
                            <Button
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Large"
                                ac:ButtonExtensions.ButtonType="Primary"
                                Content="Large Button"
                                IsDefault="True" />
                            <Button
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Large"
                                ac:ButtonExtensions.ButtonType="Primary"
                                Content="Large Button"
                                IsEnabled="False" />
                            <Button
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Medium"
                                ac:ButtonExtensions.ButtonType="Primary"
                                Content="Medium Button" />
                            <Button
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Small"
                                ac:ButtonExtensions.ButtonType="Primary"
                                Content="Small Button" />
                            <Button
                                Margin="0,5,0,0"
                              
                                Content="OK" />

                            <Slider
                                Width="250"
                                Height="Auto"
                                Margin="0,25,0,0"
                                Interval="1"
                                IsEnabled="False"
                                Maximum="100"
                                Minimum="0"
                                Value="50" />

                            <Slider
                                Width="250"
                                Height="Auto"
                                Margin="0,10,0,0"
                                Interval="1"
                                Maximum="100"
                                Minimum="0"
                                Value="50" />

                            <AdornerDecorator Margin="0,5,0,0">
                                <DatePicker
                                    x:Name="Test1"
                                    Width="120"
                                    Height="35"
                                    HorizontalContentAlignment="Stretch"
                                    VerticalContentAlignment="Center"
                                    ac:TeachingTip.InternalAdorner="{x:Null}"
                                    ac:TeachingTip.IsVisible="{Binding RelativeSource={RelativeSource Self}, Path=IsMouseOver}"
                                    ac:TeachingTip.Template="{StaticResource Test}"
                                    CalendarStyle="{StaticResource ACCalendarStyle}" />
                            </AdornerDecorator>

                        </StackPanel>
