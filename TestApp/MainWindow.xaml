                                ac:ButtonExtensions.ButtonType="Auto"
                                ac:ButtonExtensions.IconPosition="Left"
                                Background="Black"
                                Content="{StaticResource ACDelete}"
                                Foreground="Black"
                                Style="{StaticResource TextIconButtonStyle}" />

                            <Button
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Large"
                                ac:ButtonExtensions.ButtonType="Secondary"
                                ac:ButtonExtensions.Count="12"
                                ac:ButtonExtensions.CountVisibility="True"
                                Content="Large Button"
                                IsEnabled="False" />
                            <Button
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Medium"
                                ac:ButtonExtensions.ButtonType="Secondary"
                                Content="Medium Button" />
                            <Button
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Small"
                                ac:ButtonExtensions.ButtonType="Secondary"
                                Content="Small Button" />

                            <Button
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Small"
                                ac:ButtonExtensions.ButtonType="Secondary"
                                Click="Button_Click"
                                Content="Modal" />

                            <Button
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Small"
                                ac:ButtonExtensions.ButtonType="Secondary"
                                Click="Button_Click"
                                Content="Warning" />

                            <Button
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Small"
                                ac:ButtonExtensions.ButtonType="Secondary"
                                Click="Button_Click"
                                Content="Info" />

                            <Button
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Small"
                                ac:ButtonExtensions.ButtonType="Secondary"
                                Click="Button_Click"
                                Content="Error" />

                            <Button
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Small"
                                ac:ButtonExtensions.ButtonType="Secondary"
                                Click="Button_Click"
                                Content="Success" />

                            <Button
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Small"
                                ac:ButtonExtensions.ButtonType="Secondary"
                                Click="Button_Click"
                                Content="ProceedLater" />

                            <Button
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Small"
                                ac:ButtonExtensions.ButtonType="Secondary"
                                Click="Button_Click"
                                Content="SaveCancel" />
                            <Button
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Small"
                                ac:ButtonExtensions.ButtonType="Secondary"
                                Click="Button_Click"
                                Content="SaveDontSave" />

                            <Button
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Small"
                                ac:ButtonExtensions.ButtonType="Secondary"
                                Click="Button_Click"
                                Content="DeleteCancel" />

                            <Button
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Small"
                                ac:ButtonExtensions.ButtonType="Secondary"
                                Click="Button_Click"
                                Content="ShowResult" />

                            <Button
                                Width="75"
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Auto"
                                ac:ButtonExtensions.ButtonText="Button"
                                ac:ButtonExtensions.ButtonType="Auto"
                                ac:ButtonExtensions.IconPosition="Left"
                                Background="Black"
                                Content="{StaticResource ACDelete}"
                                Foreground="Black"
                                Style="{StaticResource TextIconButtonStyle}" />

                            <Button
                                Width="75"
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Auto"
