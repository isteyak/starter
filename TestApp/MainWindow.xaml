<ac:AirCloudWindow
    x:Class="TestApp.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:ac="clr-namespace:AirCloudWPF;assembly=AirCloudWPF"
    xmlns:acc ="clr-namespace:AirCloudWPF.Converters;assembly=AirCloudWPF"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
    xmlns:local="clr-namespace:TestApp"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Name="AcWin"
    Title="MainWindow"
    Width="900"
    Height="600"
    ac:WindowExtensions.Brand="{StaticResource ACHitachiBrand}"
    ac:WindowExtensions.BrandMargin="24,0,0,0"
    ac:WindowExtensions.MenuVisibility="{Binding Path=IsChecked, ElementName=Switch}"
    ac:WindowExtensions.ProjectName="{Binding ProjectName}"
    WindowState="Maximized"
    mc:Ignorable="d">
    <Window.Resources>
        <ControlTemplate x:Key="Test">
            <Grid
                Height="Auto"
                Margin="10,32,0,0"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Center">
                <Path
                    x:Name="Container"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"
                    Data="{Binding Source={StaticResource ACToolTip}}"
                    Fill="White"
                    Stretch="Fill"
                    Stroke="{StaticResource ACGrayBrush4}">
                    <Path.Effect>
                        <DropShadowEffect
                            BlurRadius="10"
                            Opacity="0.5"
                            ShadowDepth="4" />
                    </Path.Effect>
                </Path>
                <TextBlock
                    MaxWidth="250"
                    Margin="8,16,4,4"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Center"
                    Panel.ZIndex="2"
                    FontSize="14"
                    Foreground="{StaticResource ACPrimaryBlackTextBrush}"
                    Text="Hello"
                    TextWrapping="Wrap" />
            </Grid>
        </ControlTemplate>

        <ac:BooleanToErrorBrushConverter
            x:Key="ErrorBrushConverter"
            DefaultBrush="{StaticResource ACPrimaryGrayBrush}"
            ErrorColorBrush="{StaticResource ACPrimaryRedBrush}" />
        <ac:StarWidthConverter x:Key="StarWidthConverter" />
        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
        <ac:BooleanToNotVisibilityConverter x:Key="BooleanToNotVisibilityConverter" />

        <Style TargetType="{x:Type ac:AirCloudNumberBox}">
            <Setter Property="Margin" Value="0,10,0,10" />
        </Style>

        <Style x:Key="ItemStyle" TargetType="{x:Type ListViewItem}">
            <Setter Property="OverridesDefaultStyle" Value="True" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ListViewItem}">
                        <Border
                            x:Name="ItemBorder"
                            Background="Transparent"
                            BorderBrush="Transparent"
                            BorderThickness="0"
                            IsHitTestVisible="True">
                            <Grid
                                Height="30"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Background="Transparent">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="15" />
                                    <ColumnDefinition Width="Auto" MinWidth="90" />
                                    <ColumnDefinition Width="Auto" MinWidth="50" />
                                    <ColumnDefinition Width="30" />
                                </Grid.ColumnDefinitions>
                                <TextBlock
                                    Grid.Column="1"
                                    Margin="-5,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    FontSize="14"
                                    Foreground="Black"
                                    Text="{Binding UnitName}" />
                                <TextBlock
                                    Grid.Column="2"
                                    Margin="-5,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    FontSize="14"
                                    Foreground="Black"
                                    Text="{Binding Model}" />
                                <Button
                                    x:Name="DeleteButton"
                                    Grid.Column="3"
                                    Width="15"
                                    Height="15"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Background="{StaticResource ACPrimaryGrayBrush}"
                                    Content="{StaticResource ACDelete}"
                                    Cursor="Hand"
                                    Style="{StaticResource IconButtonStyle}" />
                            </Grid>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="False">
                                <Setter TargetName="ItemBorder" Property="Background" Value="Transparent" />
                                <Setter TargetName="DeleteButton" Property="Visibility" Value="Collapsed" />
                            </Trigger>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter TargetName="ItemBorder" Property="Background" Value="{StaticResource ACGrayBrush6}" />
                                <Setter TargetName="DeleteButton" Property="Visibility" Value="Visible" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>
    <ac:AirCloudWindow.Style>
        <Style TargetType="{x:Type ac:AirCloudWindow}">
            <Setter Property="ac:WindowExtensions.SideMenu">
                <Setter.Value>
                    <StackPanel
                        Background="{StaticResource ACGrayBrush6}"
                        Orientation="Horizontal"
                        WindowChrome.IsHitTestVisibleInChrome="True">
                        <StackPanel
                            Width="40"
                            Background="Transparent"
                            Orientation="Vertical"
                            Visibility="{Binding OpenMenu, Converter={StaticResource BooleanToNotVisibilityConverter}}">
                            <ToggleButton
                                x:Name="MenuButton"
                                Margin="0,-10,0,-10"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Top"
                                Background="Black"
                                Content="{StaticResource ACHamburger}"
                                IsChecked="{Binding OpenMenu}"
                                Style="{StaticResource MenuButtonStyle}" />
                            <RadioButton
                                HorizontalAlignment="Left"
                                Background="{StaticResource ACSystemDangerBrush}"
                                Command="{Binding}"
                                Content="{StaticResource ACHome}"
                                GroupName="T"
                                IsChecked="{Binding HomeChecked}"
                                Style="{StaticResource MenuRadioButtonStyle}" />
                            <RadioButton
                                HorizontalAlignment="Left"
                                Background="{StaticResource ACSystemDangerBrush}"
                                Content="{StaticResource ACSettings}"
                                GroupName="T"
                                IsChecked="{Binding SettingsChecked}"
                                Style="{StaticResource MenuRadioButtonStyle}" />
                            <RadioButton
                                HorizontalAlignment="Left"
                                Background="{StaticResource ACSystemDangerBrush}"
                                Content="{StaticResource ACTools}"
                                GroupName="T"
                                Style="{StaticResource MenuRadioButtonStyle}" />
                            <RadioButton
                                HorizontalAlignment="Left"
                                ac:ButtonExtensions.HasUpdate="True"
                                Background="{StaticResource ACSystemDangerBrush}"
                                Content="{StaticResource ACHelp}"
                                GroupName="T"
                                Style="{StaticResource MenuRadioButtonStyle}" />
                        </StackPanel>
                        <StackPanel
                            Width="250"
                            Margin="0,5,0,0"
                            Visibility="{Binding OpenMenu, Converter={StaticResource BooleanToVisibilityConverter}}">
                            <Grid Background="Transparent">
                                <i:Interaction.Triggers>
                                    <i:EventTrigger EventName="PreviewMouseDown">
                                        <i:InvokeCommandAction Command="{Binding Path=CloseMenuCommand}" />
                                    </i:EventTrigger>
                                </i:Interaction.Triggers>
                                <TextBlock
                                    Margin="5,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    FontSize="14"
                                    FontWeight="SemiBold"
                                    Text="airCloud Select" />
                                <Button
                                    Width="15"
                                    Height="20"
                                    Margin="0,0,10,0"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    Background="{StaticResource ACPrimaryGraphiteBrush}"
                                    Command="{Binding CloseMenuCommand}"
                                    Content="{StaticResource ACBack}"
                                    Style="{StaticResource IconButtonStyle}" />
                            </Grid>

                            <RadioButton
                                Width="250"
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonText="Home"
                                Content="{StaticResource ACHome}"
                                IsChecked="{Binding HomeChecked, Mode=TwoWay}"
                                Style="{StaticResource ExpanderMenuButtonStyle}" />
                            <RadioButton
                                Width="250"
                                Margin="0,0,0,0"
                                ac:ButtonExtensions.ButtonText="Settings"
                                Content="{StaticResource ACSettings}"
                                IsChecked="{Binding SettingsChecked, Mode=TwoWay}">
                                <RadioButton.Style>
                                    <Style BasedOn="{StaticResource ExpanderMenuButtonStyle}" TargetType="{x:Type RadioButton}">
                                        <Setter Property="ac:WindowExtensions.SideBarItems">
                                            <Setter.Value>
                                                <Border Margin="0,0,0,0" Background="White">
                                                    <StackPanel
                                                        Width="250"
                                                        Margin="-15,0,0,0"
                                                        HorizontalAlignment="Stretch"
                                                        Background="Transparent"
                                                        Orientation="Vertical"
                                                        WindowChrome.IsHitTestVisibleInChrome="True">
                                                        <RadioButton
                                                            x:Name="T1"
                                                            Margin="0,0,0,0"
                                                            Content="General"
                                                            Style="{StaticResource RadioButtonTextStyle}" />
                                                        <RadioButton
                                                            x:Name="T2"
                                                            Margin="0,-2.5,0,0"
                                                            Content="Location"
                                                            Style="{StaticResource RadioButtonTextStyle}" />
                                                        <RadioButton
                                                            x:Name="T3"
                                                            Margin="0,-2.5,0,0"
                                                            Content="Notifications"
                                                            Style="{StaticResource RadioButtonTextStyle}" />
                                                        <RadioButton
                                                            x:Name="T4"
                                                            Margin="0,-2.5,0,0"
                                                            Content="Measurement Unit"
                                                            Style="{StaticResource RadioButtonTextStyle}" />
                                                        <RadioButton
                                                            x:Name="T5"
                                                            Margin="0,-2.5,0,0"
                                                            Content="Name Prefixes"
                                                            Style="{StaticResource RadioButtonTextStyle}" />
                                                    </StackPanel>
                                                </Border>
                                            </Setter.Value>
                                        </Setter>
                                    </Style>
                                </RadioButton.Style>
                            </RadioButton>
                        </StackPanel>
                    </StackPanel>
                </Setter.Value>
            </Setter>

            <Setter Property="ac:WindowExtensions.UserMenu">
                <Setter.Value>
                    <Grid WindowChrome.IsHitTestVisibleInChrome="True">
                        <ToggleButton
                            x:Name="ProfileButton"
                            Content="A"
                            IsChecked="{Binding OpenUserMenu}"
                            Style="{StaticResource ProfileButtonStyle}" />
                        <Popup
                            Name="MenuList"
                            IsOpen="{Binding OpenUserMenu}"
                            Placement="Bottom"
                            PlacementTarget="{Binding ElementName=ProfileButton}">
                            <StackPanel Background="White" Orientation="Vertical">
                                <Button
                                    Width="120"
                                    Height="30"
                                    HorizontalContentAlignment="Left"
                                    VerticalContentAlignment="Center"
                                    Content="Settings" />
                                <Button
                                    Width="120"
                                    Height="30"
                                    HorizontalContentAlignment="Left"
                                    VerticalContentAlignment="Center"
                                    Content="Logout" />
                            </StackPanel>
                        </Popup>
                    </Grid>
                </Setter.Value>
            </Setter>
        </Style>
    </ac:AirCloudWindow.Style>
