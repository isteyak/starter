                            HorizontalAlignment="Left"
                            Orientation="Vertical">
                            <TextBlock Text="Secondary Buttons" />
                            <Button
                                Margin="0,5,0,0"
                                ac:ButtonExtensions.ButtonSize="Large"
                                ac:ButtonExtensions.ButtonType="Secondary"
                                ac:ButtonExtensions.Count="12"
                                ac:ButtonExtensions.CountVisibility="True"
